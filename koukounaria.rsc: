/ip dhcp-client add interface=ether1 disabled=no
/interface wireless cap set enabled=no
/interface wireless reset-configuration wlan1
/interface wireless reset-configuration wlan2
/interface wireless security-profiles add name=open authentication-types="" unicast-ciphers="" group-ciphers=""
/interface wireless set wlan1 mode=ap-bridge ssid="Koukounaria Guest 9" security-profile=open disabled=no
/interface wireless set wlan2 mode=ap-bridge ssid="Koukounaria Guest 9" security-profile=open disabled=no
/interface bridge print
/interface bridge port add bridge=bridge1 interface=wlan1
/interface bridge port add bridge=bridge1 interface=wlan2
/interface bridge vlan add bridge=bridge1 vlan-ids=10 tagged=bridge1,ether1,wlan1,wlan2
/interface bridge set [find name=bridge1] vlan-filtering=yes
/system reboot without-prompt=yes